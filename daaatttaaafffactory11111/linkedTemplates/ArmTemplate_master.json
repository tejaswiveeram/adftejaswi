{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "daaatttaaafffactory11111"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"filenamedatabaselinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'filenamedatabaselinkedservice'"
		},
		"linkedservice_for_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_for_source'"
		},
		"linkedservice_for_target_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_for_target'"
		},
		"ls_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_gen2'"
		},
		"ls_gen2_output_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_gen2_output'"
		},
		"ls_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://instancecce.dfs.core.windows.net/"
		},
		"ls_gen2_output_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://instancecce.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "daaatttaaafffactory11111_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"filenamedatabaselinkedservice_connectionString": {
						"value": "[parameters('filenamedatabaselinkedservice_connectionString')]"
					},
					"linkedservice_for_source_connectionString": {
						"value": "[parameters('linkedservice_for_source_connectionString')]"
					},
					"linkedservice_for_target_connectionString": {
						"value": "[parameters('linkedservice_for_target_connectionString')]"
					},
					"ls_gen2_accountKey": {
						"value": "[parameters('ls_gen2_accountKey')]"
					},
					"ls_gen2_output_accountKey": {
						"value": "[parameters('ls_gen2_output_accountKey')]"
					},
					"ls_gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_gen2_properties_typeProperties_url')]"
					},
					"ls_gen2_output_properties_typeProperties_url": {
						"value": "[parameters('ls_gen2_output_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}